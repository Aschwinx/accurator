@prefix auis:   <http://accurator.nl/ui/schema#> .
@prefix aui:    <http://accurator.nl/ui/generic#> .
@prefix abirui: <http://accurator.nl/ui/bird#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:<http://purl.org/dc/terms/> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .

###### Bird Domain #####
abirui:domain a auis:UI ;
	aui:domainLabel "Bird domain"@en ;
	aui:domainLabel "Vogel domein"@nl .

aui:domainLabel a auis:UILabel .

###### Bird Intro Screen #####

abirui:intro a auis:UI ;
	rdfs:subClassOf aui:intro;
	aui:txtSubSlogan "Help us add information to artworks depicting birds"@en ;
	aui:txtSubSlogan "Help ons met het toevoegen van informatie aan kunstwerken met vogel afbeeldingen"@nl .

abirui:txtSubSlogan a auis:UILabel .

###### Bird About Screen #####

abirui:about a auis:UI ;
	rdfs:subClassOf aui:about .

##### Bird Register Screen #####

abirui:register a aui:UI ;
	rdfs:subClassOf aui:register .

##### Bird Expertise Screen #####

abirui:expertise a auis:UI ;
	rdfs:subClassOf aui:expertise ;
	aui:txtSubHeader "tell us which of these bird species you know much about, so we can recommend interesting works to add information to"@en ;
	aui:txtSubHeader "vertel ons over welke vogelsoorten u veel weet, dan baseren wij de werken die we aanraden hierop"@nl .

abirui:txtSubHeader a auis:UILabel .

##### Bird Profile Screen #####

abirui:profile a aui:UI ;
	rdfs:subClassOf aui:profile .

##### Bird Results Screen #####

abirui:results a aui:UI ;
	rdfs:subClassOf aui:results .

##### Bird Annotate Screen #####

abirui:annotate a aui:UI ;
	rdfs:subClassOf aui:annotate .

##### Bird Additional Info Screen #####

abirui:additional_info a auis:UI ;
	rdfs:subClassOf aui:additional_info .

##### Bird Login Modal #####

abirui:login_modal a auis:UI ;
	rdfs:subClassOf aui:login_modal .
